    {
        "id": "b67cdc54c42e74ca",
        "type": "function",
        "z": "8296850d5b2a0086",
        "name": "Parse Telemetry -> Ws",
        "func": "let data = {};\ntry {\n    data = (typeof msg.payload === 'string') ? JSON.parse(msg.payload) : msg.payload;\n} catch {\n    return null;\n}\n\nconst device_id = (msg.topic || '').split(\"/\")[1];\nif (!device_id) return null;\n\nconst obj = {\n    _id: msg.payload._id,\n    device_id,\n    sensor: msg.payload.sensor || 'unlnown',\n    unit: msg.payload.unit || null,\n    value: msg.payload.value || null,\n    created_at: new Date(msg.payload.ts || Date.now()),\n    sourcer: 'telemetry'\n}\n\nconst reading = {\n    collection: 'reading',\n    operation: 'insertOne',\n    payload: obj\n}\n\nconst device = {\n    collection: 'devices',\n    operation: 'updateOne',\n    payload: [\n        { device_id },\n        { $set: { 'status.last_seen': new Date(), updated_at: new Date() }, $setOnInsert: { created_at: new Date(), device_id, _id:msg.payload._id } },\n        { upsert: true }\n    ]\n}\n\nreturn [device, reading];\n",
