        "z": "1e3fa613f886d610",
        "name": "Create Commands",
        "func": "const deviceId = msg.req.params.id;\nconst command = msg.req.body; \n\nconst value = (typeof command.on === 'boolean') ? { on: command.on } : { angle: + command.angle };\n\nconst commandDb = {\n    _id: msg.payload._id,\n    deviceId: deviceId,\n    target: command.target,\n    payload: value,\n    corr: command.corr,\n    request_by: 'dashboard',\n    request_at: new Date(),\n    ack: null,\n    status: 'sent',\n    delivery: {\n        sent_ts: new Date()\n    },\n    error: null\n}\n\nconst msgInsert = {\n    collection: \"commands\",\n    operation: \"insertOne\",\n    payload: [commandDb]\n}\n\nreturn [msgInsert, null, null];",
